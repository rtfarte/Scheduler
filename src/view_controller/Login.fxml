        <Label fx:id="usernameLabel" layoutX="62.0" layoutY="80.0" text="Username" wrapText="true" AnchorPane.leftAnchor="62.0" AnchorPane.rightAnchor="284.0" />
        <Label fx:id="passwordLabel" layoutX="64.0" layoutY="124.0" text="Password" wrapText="true" AnchorPane.leftAnchor="62.0" AnchorPane.rightAnchor="284.0" />
        <Label fx:id="loginLabel" layoutX="31.0" layoutY="14.0" prefHeight="17.0" prefWidth="93.0" text="Login" wrapText="true" AnchorPane.topAnchor="14.0" />
        <TextField fx:id="usernameTextField" layoutX="135.0" layoutY="76.0" prefHeight="25.0" prefWidth="221.0" AnchorPane.rightAnchor="44.0" />
        <TextField fx:id="passwordTextField" layoutX="135.0" layoutY="120.0" prefHeight="25.0" prefWidth="221.0" AnchorPane.rightAnchor="44.0" />

    </children>
</AnchorPane>
